<?xml version="1.0" encoding="UTF=8"?>
<!DOCTYPE Books [
    <!ELEMENT Books (book+)>
    <!ELEMENT book (title?,isbn+,(zipcode|author),publish*)>
    <!ELEMENT title #PCDATA>
    <!ELEMENT isbn #PCDATA>
    <!ELEMENT zipcode #PCDATA>
    <!ELEMENT author #PCDATA>
    <!ELEMENT publish #PCDATA>
    <!ATTLIST title name #CDATA #REQUIRED>
    <!ATTLIST isbn no #CDATA #REQUIRED>
    <!ATTLIST zipcode zname DEFAULT '232232323'>
    <!ATTLIST publish year #CDATA FIXED '1995'>
]>


<Books>
    <book>
        <title name='abcd'>1
        <title>
        <title>2
        <title>
        <isbn no='1234'>12345
        <isbn>
        <zipcode zname='232232323'>12345
        <zipcode>
        <publish year='1995'>Nepal
        <publish>
    </book>

    <book>
        <title>1
        <title>
        <title>2
        <title>
        <isbn>12345
        <isbn>
        <zipcode>12345
        <zipcode>
        <publish>Nepal
        <publish>
    </book>
</Books>